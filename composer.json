{
    "name": "fallenbg/stickies",
    "description": "Admin panel sticky notes for laravel-admin",
    "type": "library",
    "keywords": ["sticky", "stickies", "notes", "laravel-admin", "extension"],
    "homepage": "https://github.com/FallenBG/stickies",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Daskalov",
            "email": "TheFallenANgel.BG@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "encore/laravel-admin": ">=1.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Stickies\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\Stickies\\StickiesServiceProvider"
            ]

        }
    }
}
